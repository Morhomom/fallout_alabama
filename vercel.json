{
  "rewrites": [
    {
      "source": "/api/overseer/:path*",
      "destination": "/api/overseer.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/frontend/:path*"
    }
  ]
}